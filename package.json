{
  "name": "sonos-controller-for-chrome",
  "version": "0.0.1",
  "description": "Sonos Controller for Chrome",
  "main": "dist/background.js",
  "scripts": {
    "watch": "npm run-script watch-ui & npm run-script watch-bg",
    "watch-ui": "./node_modules/.bin/watch \"npm run-script compile-ui && npm run-script minify-ui\" src/ui",
    "watch-bg": "./node_modules/.bin/watch \"npm run-script compile-bg && npm run-script minify-bg\" src/background",
    "minify-ui": "node_modules/.bin/browserify --extension=.jsx -e tmp/ui/app.js > js/ui.js",
    "minify-bg": "node_modules/.bin/browserify -e tmp/background/background.js > js/background.js",
    "compile-ui": "node_modules/.bin/6to5 src/ui --out-dir=tmp/ui -m common -a",
    "compile-bg": "node_modules/.bin/6to5 src/background --out-dir=tmp/background -m common -a",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "6to5": "^2.2.0",
    "array-union": "^1.0.0",
    "bloody-events": "^1.3.0",
    "browserify": "^8.1.1",
    "deep-equal": "^0.2.1",
    "lodash": "^3.0.0",
    "omit-keys": "^0.1.0",
    "react": "^0.12.2",
    "react-cursor": "^0.9.2",
    "watch": "^0.13.0",
    "jquery-xml2json": "git+https://github.com/pascalopitz/jQuery-xml2json.git"
  }
}
