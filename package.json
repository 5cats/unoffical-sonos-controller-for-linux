{
  "name": "sonos-controller-for-chrome",
  "version": "0.0.18",
  "description": "Sonos Controller for Chrome",
  "main": "app/main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build",
    "develop": "npm run fonts && npm run transpile && npm run start",
    "start": "electron .",
    "watch": "babel -d ./app/js -s true -w ./src",
    "transpile": "babel -d ./app/js -s true ./src",
    "fonts": "mkdir -p ./app/fonts && cp ./node_modules/material-design-icons/iconfont/* ./app/fonts/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Pascal Opitz",
    "email": "contact@pascalopitz.com",
    "url": "http://github.com/pascalopitz"
  },
  "contributors": [
    {
      "name": "leanto",
      "url": "https://github.com/leanto"
    }
  ],
  "build": {
    "appId": "com.electron.unoffcial.sonos.controller",
    "app-category-type": "Audio",
    "linux": {
      "target": "deb"
    }
  },
  "license": "ISC",
  "devDependencies": {
    "array-union": "^1.0.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.14.0",
    "deep-equal": "^0.2.2",
    "electron": "^1.4.1",
    "electron-builder": "^7.9.0",
    "eslint": "^1.10.3",
    "flux": "^2.1.1",
    "jquery-xml2json": "git+https://github.com/pascalopitz/jQuery-xml2json.git",
    "lodash": "^3.10.1",
    "material-design-icons": "^2.2.3",
    "moment": "^2.13.0",
    "omit-keys": "^0.1.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-draggable2": "^0.7.0-alpha1",
    "uuid": "^2.0.2",
    "watch": "^0.13.0"
  },
  "dependencies": {
    "request": "^2.75.0"
  }
}
