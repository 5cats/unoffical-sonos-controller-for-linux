{
  "retainLines": true,
  "compact": false,
  "comments": false,
  "presets": [
    "node5",
    "react"
  ],
  "plugins": [
    "syntax-async-functions",
    ["transform-react-jsx", { "pragma":"h" }],
    ["transform-async-to-module-method", {
        "module": "bluebird",
        "method": "coroutine"
    }]
  ],
  "sourceMaps": true
}
